datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model users {
  id             String @id @default(auto()) @map("_id") @db.ObjectId
  workspaceName  String
  email          String?  @unique
  address        String? 
}